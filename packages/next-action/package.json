{
  "name": "next-action",
  "version": "0.0.1-beta2",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src",
    "format": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "test": "cross-env NODE_ENV=development vitest --run",
    "prepublish": "pnpm run typecheck && pnpm run build"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "jsdom": "^24.0.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "seria": "0.1.9",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "next": "14.1.0",
    "react": "^18.2.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "default": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "default": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "default": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./testing/server": {
      "import": "./dist/testing/server.js",
      "default": "./dist/testing/server.js",
      "types": "./dist/testing/server.d.ts"
    },
    "./testing/client": {
      "import": "./dist/testing/client.js",
      "default": "./dist/testing/client.js",
      "types": "./dist/testing/client.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "./package.json"
  ]
}
